pgr[DimA]=
	(p[DimA] - SMOOTH(p[DimA], p delay)) / p[DimA] / p delay
	~
	~		|

DimA:
	A1, A2, A3
	~
	~		|

p delay=
	1
	~
	~		|

p[A2]=
	2000 + RAMP(100, 1, 10) ~~|
p[A1]=
	1000 + RAMP(100, 1, 10) ~~|
p[A3]=
	3000 + RAMP(100, 1, 10)
	~
	~		|

delay=
	2
	~
	~		|

input=
	3 + PULSE(10, 10)
	~
	~		|

S2 Level= INTEG (
	(input - S2 Level) / delay,
		input)
	~
	~		|

S1=
	scale * SMOOTH(input, delay)
	~
	~		|

S2=
	scale * S2 Level
	~
	~		|

scale=
	6
	~
	~		|

